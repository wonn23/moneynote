{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest Framework",
      "runtimeArgs": ["-r", "ts-node/register"],
      "args": [
        "-r",
        "tsconfig-paths/register",
        "${workspaceFolder}/src/main.ts"
      ],
      "autoAttachChildProcesses": true,

      "console": "integratedTerminal"
    }
  ]
}
